{
  "name": "myapp",
  "version": "1.0.0",
  "description": "My cool app",
  "main": "index.js",
  "scripts": {
    "compile": "node build.js && backpack build",
    "lint": "eslint -c .eslintrc.json --ext .js ./test ./server",
    "start": "cd build && node main | pino-socket -a $LOGGING_HOST -p $LOGGING_PORT -m tcp",
    "start:dev": "node build.js && backpack dev | pino -o 'short'",
    "start:debug": "node build.js && backpack dev --inspect-brk | pino -o 'short'",
    "test": "node build.js && _mocha --compilers js:babel-register --exit"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "pino": "^4.7.3",
    "pino-socket": "^1.0.1",
    "source-map-support": "^0.4.15",
    "swagger-express-middleware": "^1.0.0-alpha.12"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.6.1",
    "backpack-core": "^0.4.3",
    "chai": "^4.1.2",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-mocha": "^4.11.0",
    "mocha": "^4.0.1",
    "shelljs": "^0.7.8",
    "supertest": "^3.0.0"
  },
  "author": "David COHEN <dcohen@xyz.zone> @ LaZone iA",
  "engines": {
    "node": "8.9"
  }
}
